{
    "name": "fluent-sql",
    "version": "1.0.0",
    "description": "Fluent SQL lib",
    "main": "src/fluent-sql.js",
    "directories": {
        "test": "test"
    },
    "dependencies": {
        "sprintf-js": "^1.0.3",
        "sliced": "^0.0.5"
    },
    "devDependencies": {
        "blanket": "^1.1.7",
        "babel-core": "^5.8.25",
        "babel": "^5.8.23",
        "chai": "^3.3.0",
        "babel-loader": "^5.3.2",
        "es5-shim": "^4.1.13",
        "es6-shim": "^0.28.1",
        "karma": "^0.13.10",
        "karma-es6-shim": "^0.1.3",
        "karma-coverage": "^0.5.2",
        "karma-mocha": "^0.2.0",
        "webpack": "^1.12.2"
    },
    "scripts": {
        "oldtest": "./node_modules/.bin/mocha --reporter spec",
        "test": "./node_modules/.bin/mocha --compilers js:babel/register --reporter spec",
        "coverage": "./node_modules/.bin/mocha --compilers js:babel/register --require blanket -R html-cov > coverage.html"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jefflindholm/fluent-sql.git"
    },
    "keywords": [
        "SQL",
        "Fluent"
    ],
    "author": "Jeff Lindholm <jeff@lindholm.org>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/jefflindholm/fluent-sql/issues"
    },
    "homepage": "https://github.com/jefflindholm/fluent-sql#readme",
    "config": {
        "blanket": {
            "pattern": [ "/src/" ],
            "data-cover-never": [
                "node_modules",
                "tests"
            ]
        }
    }
}
