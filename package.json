{
  "name": "fluent-sql",
  "version": "1.3.14",
  "description": "Fluent SQL lib",
  "main": "dist/fluent-sql.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "do-publish": "npm publish --registry=https://registry.npmjs.org",
    "precommit": "npm run test:single && rm -rf dist",
    "prebuild": "shx rm -rf dist && mkdir dist",
    "build": "babel --presets es2015,stage-0 -d dist/ src/",
    "prepublish": "npm run build",
    "test": "./node_modules/.bin/mocha --require babel-core/register --reporter spec -w",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --  --require babel-core/register -R spec",
    "test:single": "./node_modules/.bin/mocha --require babel-core/register",
    "debug": "start node-inspector && mocha --debug-brk  --require babel-core/register --reporter spec",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "lint": "eslint src/*.js"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "circular-json": "^0.3.1",
    "cz-conventional-changelog": "^1.2.0",
    "es5-shim": "^4.5.8",
    "es6-shim": "^0.35.0",
    "eslint": "^3.7.0",
    "eslint-plugin-mocha": "^4.5.1",
    "findup": "^0.1.5",
    "husky": "^0.11.8",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.0",
    "semantic-release": "^4.3.5",
    "shelljs": "^0.7.4",
    "shx": "^0.1.4",
    "sprintf-js": "^1.0.3",
    "webpack": "^1.13.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jefflindholm/fluent-sql.git"
  },
  "keywords": [
    "SQL",
    "Fluent"
  ],
  "author": "Jeff Lindholm <jeff@lindholm.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jefflindholm/fluent-sql/issues"
  },
  "homepage": "https://github.com/jefflindholm/fluent-sql#readme",
  "config": {
    "blanket": {
      "pattern": [
        "/src/"
      ],
      "data-cover-never": [
        "node_modules",
        "tests"
      ]
    }
  }
}
