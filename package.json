{
  "name": "fluent-sql",
  "version": "1.3.5",
  "description": "Fluent SQL lib",
  "main": "dist/fluent-sql.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "commit": "git-cz",
    "prebuild": "shx rm -rf dist && mkdir dist",
    "build": "babel --presets es2015,stage-0 -d dist/ src/",
    "prepublish": "npm run build",
    "test": "./node_modules/.bin/mocha --require babel-core/register --reporter spec -w",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/mocha/bin/_mocha --  --require babel-core/register -R spec",
    "test:single": "./node_modules/.bin/mocha --require babel-core/register",
    "debug": "start node-inspector && mocha --debug-brk  --require babel-core/register --reporter spec",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "lint": "eslint src/*.js"
  },
  "dependencies": {
    "util": "^0.10.3"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "blanket": "^1.2.3",
    "chai": "^3.5.0",
    "circular-json": "^0.3.0",
    "commitizen": "^2.8.0",
    "cz-conventional-changelog": "^1.1.5",
    "es5-shim": "^4.5.8",
    "es6-shim": "^0.35.0",
    "eslint": "^2.11.1",
    "eslint-plugin-mocha": "^2.2.0",
    "ghooks": "^1.2.1",
    "istanbul": "^0.4.3",
    "mocha": "^2.4.5",
    "semantic-release": "^4.3.5",
    "shelljs": "^0.7.0",
    "shx": "^0.1.2",
    "webpack": "^1.13.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jefflindholm/fluent-sql.git"
  },
  "keywords": [
    "SQL",
    "Fluent"
  ],
  "author": "Jeff Lindholm <jeff@lindholm.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jefflindholm/fluent-sql/issues"
  },
  "homepage": "https://github.com/jefflindholm/fluent-sql#readme",
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single && rm -rf dist"
    },
    "blanket": {
      "pattern": [
        "/src/"
      ],
      "data-cover-never": [
        "node_modules",
        "tests"
      ]
    }
  }
}
